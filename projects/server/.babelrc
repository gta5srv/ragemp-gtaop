{
  "presets": [
    "@babel/preset-typescript",
    "@babel/preset-env"
  ],
  "plugins": [
    ["module-resolver", {
      "cwd": "./src",
      "alias": {
        "@root": "./",
        "@lib": "./lib",
        "@core": "./lib/core",
        "@config": "./config",
        "@assets": "./assets"
      }
    }],
    "@babel/plugin-proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    "@babel/plugin-transform-modules-commonjs"
  ]
}
